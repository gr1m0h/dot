[user]
  name = grimoh
  email = dev@grimoh.net
[core]
  editor = nvim -c \"set fenc=utf-8\"
  diff = auto
  status = auto
  branch = auto
[pull]
  rebase = false
[commit]
  gpgsign = true
[alias]
  alias = !git config --get-regexp '^alias\\.' | sed 's/alias\\.\\([^ ]*\\) \\(.*\\)/\\1\\ => \\2/' | sort
  a = !git add -A && git status
  au = !git add -u && git status
  cam = commit --amend
  wip = commit -m "wip"
  cm = commit
  st = status
  sw = switch
  swc = switch -c
  graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  cancel = reset --soft HEAD^
  merge-plan = merge --no-commit --no-ff
[ghq]
  root = /Users/gr1m0h/Documents/workspace
[gpg]
  format = ssh
[include]
  path = /opt/homebrew/opt/dracula-git/gitconfig
