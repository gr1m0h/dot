{
  "mode": "replace",
  "agents": ["claude-code", "claude-desktop"],
  "mcpServers": {
    "context7": {
      "command": "/Users/gr1m0h/.local/share/mise/shims/npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "env": {}
    },
    "serena": {
      "command": "/Users/gr1m0h/.local/share/mise/shims/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ],
      "env": {}
    },
    "gcloud": {
      "command": "/Users/gr1m0h/.local/share/mise/shims/npx",
      "args": ["-y", "@google-cloud/gcloud-mcp"],
      "env": {}
    },
    "aws-mcp": {
      "command": "/Users/gr1m0h/.local/share/mise/shims/uvx",
      "timeout": 100000,
      "transport": "stdio",
      "args": [
        "mcp-proxy-for-aws@latest",
        "https://aws-mcp.us-east-1.api.aws/mcp",
        "--metadata",
        "AWS_REGION=ap-northeast-1"
      ]
    }
  }
